{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "Lua.format.defaultConfig": {
        "call_parentheses": "Always",
        "column_width": "120",
        "indent_type": "Spaces",
        "indent_width": "4",
        "line_endings": "Unix",
        "quote_style": "AutoPreferDouble"
    }
}
